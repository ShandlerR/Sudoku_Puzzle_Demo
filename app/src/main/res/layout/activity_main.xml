        android:maxLength="1"
        android:text="5"
        android:textAlignment="center"
        android:textSize="48sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textBox1"
        tools:text="7" />

    <EditText
        android:id="@+id/textBox1"
        android:layout_width="70dp"
